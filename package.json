{
  "name": "frontend-flow",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client": "webpack serve --env port=3000",
    "start:server": "nodemon ./server/server.ts",
    "start:storybook": "npx storybook dev --config-dir config/storybook --port 6006 --no-open ",
    "build:dev": "webpack build --env mode=development",
    "build:prod": "webpack build --env mode=production host=https://api-prod.com",
    "build:storybook": "npx storybook build --config-dir config/storybook",
    "lint:ts": "npx eslint --ext .ts,.tsx src/",
    "lint:ts:fix": "npx eslint --ext .ts,.tsx src/ --fix",
    "lint:css": "npx stylelint '**/*.scss'",
    "lint:css:fix": "npx stylelint '**/*.scss' --fix",
    "test:ts": "jest --config ./config/jest/jest.config.ts",
    "test:ts:coverage": "jest --config ./config/jest/jest.config.ts --coverage",
    "test:ui": "npx loki test",
    "test:ui:approve": "npx loki approve",
    "test:ui:ci": "npm run build:storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "prepare": "husky install",
    "dependencies:update": "npx ncu --interactive --format group"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
