{
  "name": "frontend-flow",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:client": "webpack serve --env port=3000",
    "start:dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "nodemon ./server/server.ts",
    "start:storybook": "npx storybook dev --config-dir config/storybook --port 6006 --no-open",
    "build:dev": "webpack build --env mode=development",
    "build:prod": "webpack build --env mode=production host=https://api-prod.com",
    "build:storybook": "npx storybook build --config-dir config/storybook",
    "test:ci": "npm run build:storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "test:cypress": "npx cypress open",
    "test:eslint:fix": "npx eslint --ext .ts,.tsx src/ --fix",
    "test:eslint": "npx eslint --ext .ts,.tsx src/",
    "test:jest:coverage": "jest --config ./config/jest/jest.config.ts --coverage",
    "test:jest": "jest --config ./config/jest/jest.config.ts",
    "test:loki:approve": "npx loki approve",
    "test:loki": "npx loki test",
    "test:stylelint:fix": "npx stylelint '**/*.scss' --fix",
    "test:stylelint": "npx stylelint '**/*.scss'",
    "prepare": "husky install",
    "packages:update": "npx ncu --interactive --format group"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
