{
  "name": "advance-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "build:dev": "webpack build --env mode=development",
    "build:prod": "webpack build --env mode=production",
    "lint:ts": "npx eslint --ext .ts,.tsx src/",
    "lint:ts:fix": "npx eslint --ext .ts,.tsx src/ --fix",
    "lint:css": "npx stylelint '**/*.scss'",
    "lint:css:fix": "npx stylelint '**/*.scss' --fix",
    "test:ts": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npm run storybook:build && npm run test:ui:start",
    "test:ui:start": "loki --requireReference --reactUri file:./storybook-static",
    "test:ui:approve": "loki approve",
    "storybook:start": "start-storybook -p 6006 --no-open --config-dir config/storybook",
    "storybook:build": "build-storybook --config-dir config/storybook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
