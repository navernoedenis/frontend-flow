{
  "name": "frontend-flow",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client": "webpack serve --env port=3000",
    "start:server": "nodemon ./server/server.ts",
    "start:storybook": "start-storybook -p 6006 --no-open --config-dir config/storybook",
    "build:dev": "webpack build --env mode=development",
    "build:prod": "webpack build --env mode=production host=https://api-prod.com",
    "build:storybook": "build-storybook --config-dir config/storybook",
    "lint:ts": "npx eslint --ext .ts,.tsx src/",
    "lint:ts:fix": "npx eslint --ext .ts,.tsx src/ --fix",
    "lint:css": "npx stylelint '**/*.scss'",
    "lint:css:fix": "npx stylelint '**/*.scss' --fix",
    "test:ts": "jest --config ./config/jest/jest.config.ts",
    "test:ts:coverage": "jest --config ./config/jest/jest.config.ts --coverage",
    "test:ui": "npx loki test",
    "test:ui:approve": "npx loki approve",
    "test:ui:ci": "npm run build:storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "prepare": "husky install",
    "update:dependencies": "npx ncu --interactive --format group"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.5",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^12.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-virtuoso": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^6.5.1",
